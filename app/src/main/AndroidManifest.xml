<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.algomusicia">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/algo_app_logo"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/algo_app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme1">
        <activity android:name=".PlayerActivity_final"
            android:theme="@style/AppTheme"
          >


        </activity>
        <activity android:name=".StartingActivity">

        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".PlayerActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".EqualiserProcess" />
        <activity android:name=".EqualiserProcess_Final"
            android:theme="@style/AppTheme"/>



        <activity
            android:name=".MainActivity2"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme1">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>